{
  "name": "Example Workflow",
  "description": "Example configuration",
  "version": "1.0.0",
  "userId": "user123",
  "clientConfig": {
    "name": "Example Client"
  },

  "workflow": {
    "taskOrder": "sequential",
    "tasks": [
      {
        "name": "task1",
        "prompt": "Complete the general example task.",
        "evaluate": "after-task"
      },
      {
        "name": "task1",
        "prompt": "Complete the second example task.",
        "evaluate": "after-workflow"
      }
    ]
  },

  "resources": {
    "runtime": {
      "language": "python",
      "version": "3.8",
      "dependencies": ["numpy", "pandas"],
      "config": {
        "memory": "512MB",
        "timeout": 300
      }
    },

    "datasources": [
      {
        "name": "db2",
        "type": "postgresql",
        "config": {
          "host": "localhost",
          "port": 5432,
          "username": "user",
          "password": "pass",
          "database": "db2"
        }
      },
      {
        "name": "redis",
        "type": "redis",
        "config": {
          "host": "localhost",
          "port": 6379,
          "password": "pass"
        }
      }
    ]
  },

  "planning": {
    "active": true,
    "planningMethod": "agile",
    "defaultSteps": ["step1", "step2", "step3"],
    "agents": {
      "shouldFind": true,
      "canCreate": true,
      "minReviewScore": 0.5,
      "defaults": [
        {
          "name": "agent1",
          "type": "type1",
          "source": "https://example.com/agent1",
          "config": {
            "param1": "value1",
            "param2": "value2"
          }
        },
        {
          "name": "agent2",
          "type": "type2",
          "source": "~./agents/agent2",
          "config": {
            "param1": "value1",
            "param2": "value2"
          }
        }
      ]
    }
  },

  "evaluation": {
    "active": true,
    "generateReport": true,
    "createAgentReviews": true,
    "reportDestinations": [
      {
        "type": "email",
        "email": "example@example.com",
        "maxTokens": 2000,
        "format": "pdf",
        "responseLanguage": "en",
        "responseComplexity": "high"
      }
    ],
    "onCompletion": "end"
  }
}
